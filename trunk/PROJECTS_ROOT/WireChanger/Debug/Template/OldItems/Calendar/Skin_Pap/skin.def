<COMMON>
ITEM-NAME:(Paper)
CELL-SPACING:(2)
CALENDAR-TITLE:( [dd MMMM, dddd])
CALENDAR-BACKGROUND:(WIDTH-LEFT-OFFSET)
USE-OOI-CELLS:(1)
</COMMON>

<TITLE>
TEXT-CONCATE:( $=<IF:$=<PAR:BIRTHDAYS>==1?$=<STRXTR-BEGIN:"; ">$=<TEXTLINE(Profile:birthdesc.txt,'$=<DAY>/$=<MONTH> ')>$=<STRXTR>$=<TEXTLINE(Profile:birth_l$=<LANG>.txt,'$=<DAY>/$=<MONTH> ')>$=<STRXTR>$=<TEXTLINE(Profile:dates_l$=<LANG>.txt,'$=<DAY>/$=<MONTH> ')>$=<STRXTR>$=<TEXTLINE(Profile:dates_l$=<LANG>.txt,'$=<WEEKDAY>_$=<WEEKNUM>/$=<MONTH> ')>$=<STRXTR-END>/IF>)
HALIGN:(LEFT)
VALIGN:(BOTTOM)
IMAGE-LBORDER:(1)
IMAGE-RBORDER:(1)
IMAGE-BORDERS:(5)
SHADOW:(1,M)
OPACITY:($=<INTEXPR($=<PAR:CLN_OPACITY>+10)>)
BG-COLOR:($=<PAR:CLN_BG>)
TEXT-COLOR:($=<PAR:CLN_FG>)
TEXT-RECT-HEIGHT:(24)
FONT:($=<PAR:CLN_FST>)
HINTS:(FULL-HEIGHT-BG,BG-NORECT)
</TITLE>

<SUBTITLE>
HALIGN:(CENTER)
SHADOW:(1,S,M)
OPACITY:($=<INTEXPR($=<PAR:CLN_OPACITY>+10)>)
BG-COLOR:($=<PAR:CLN_BG>)
TEXT-COLOR:($=<PAR:CLN_FG>)
FONT:($=<PAR:CLN_FSTS>)
TEXT-RECT-HEIGHT:(20)
HINTS:(BG-NORECT)
</SUBTITLE>

<DAYWEEK-NAME-HL>
TEXT-COLOR:($=<PAR:CLN_HL>)
<DAYWEEK-NAME>
HALIGN:(CENTER)
SHADOW:(1,zS,M)
