function FindProxyForURL(url, host)
{
  if (isPlainHostName(host) ||
    isInNet(host, &#34;128.1.0.0&#34;, &#34;255.255.0.0&#34;) || 
    isInNet(host, &#34;195.245.232.11&#34;, &#34;255.255.255.255&#34;) || 
    isInNet(host, &#34;10.0.0.0&#34;, &#34;255.0.0.0&#34;) )
    return &#34;DIRECT&#34;;
  else
    return &#34;PROXY 10.1.100.100:8080; DIRECT&#34;;
}";
